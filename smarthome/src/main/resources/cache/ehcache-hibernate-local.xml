<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" name="hibernateCache">

	<diskStore path="java.io.tmpdir/ehcache/cmcp/hibernate" />

	<!-- DefaultCache setting. -->
	<defaultCache maxEntriesLocalHeap="10000" eternal="false" timeToIdleSeconds="30000000" timeToLiveSeconds="6000000"
		overflowToDisk="true" maxEntriesLocalDisk="100000" />
		
</ehcache>
