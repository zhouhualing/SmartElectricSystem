<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.harmazing.mapper.AirconditionMapper">
	<resultMap id="ACResultMap"  type="AirCondition">
		<result property="id"		column="id" />
		<result property="type"		column="type" />
		<result property="mac" 		column="mac" />
		<result property="sn"		column="sn" />
		<result property="hardwareVer" column="hardware" />
		<result property="softwareVer" column="software" />
		<result property="currentSoftwareVer" column="cursoft" />
		<result property="disabled"	column="disable" />
		<result property="status"	column="status" />
		<result property="operStatus"  column="operStatus" />
		<result property="onOffStatus" column="onOff" />
		<result property="mode"	column="mode" />
		<result property="gwId"		column="gw_id" />
		<result property="targetTemp" column="temp" />
		<result property="indoorTemp" column="acTemp" />
		<result property="speed" 	 column="speed" />
		<result property="direction" column="direction" />
		<result property="accumulatedPower" column="accumulatePower" />
		<result property="acType"	column="acType" />	
